{
  "name": "snowmen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "next dev --port $DEV_PORT",
    "build": "next build && next export -o /home/$USER/www/akron-snowmen",
    "format": "fldev format",
    "format:check": "fldev format --check",
    "ts:check": "fldev ts:check",
    "compile": "true",
    "test": "true",
    "lint": "fldev lint --rule '@typescript-eslint/quotes: off'",
    "lint:fix": "fldev lint:fix",
    "storybook": "start-storybook --static-dir ./public --port $SB_PORT --ci",
    "build-storybook": "build-storybook --output-dir /home/$USER/www/storybook-snowmen",
    "prettier:watch": "onchange \"./**/*.{ts,tsx,js,css}\" -- prettier --config ./node_modules/@friends-library/dev/.prettierrc.json --write --ignore-unknown {{changed}}"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "next": "^10.0.0",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@friends-library/dev": "^4.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "onchange": "^7.1.0",
    "react-is": "^16.13.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "bash ./.pre-commit.sh"
    }
  }
}
